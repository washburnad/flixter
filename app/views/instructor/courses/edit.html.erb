<div class="booyah-box col-xs-10 col-xs-offset-1">
	<h1> Edit Course </h1>
	<%= simple_form_for current_course do |f| %>
		<%= f.input :title %>
		<%= f.input :description %>
		<%= f.input :cost %>
		<%= f.input :image %>
		<br />
		<%= f.submit 'Save Changes', :class => 'btn btn-primary' %>
		<br />
	<% end %>
</div>